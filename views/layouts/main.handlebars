<!-- Modal Trigger -->
<a class="waves-effect waves-light btn modal-trigger" href="#modal1">Modal</a>

<!-- Modal Structures -->
<div id="modal1" class="modal">
  <div class="modal-content">
    <h4>Login Header</h4>
    <p>A bunch of text</p>
  </div>
  <div class="modal-footer">
    <a href="/" class="modal-action modal-close waves-effect waves-green btn-flat">Agree</a>
  </div>
</div>

<!-- Modal Trigger -->
<a class="waves-effect waves-light btn modal-trigger" href="#loginModal">Login</a>

<!-- Modal Structures -->
<div id="loginModal" class="modal">
  <div class="modal-content">
    <h4>Login Header</h4>
    <p>A bunch of text</p>
  </div>

    <div class="container">
      <div class="row">
        <div class="col-md-6 col-md-offset-3">
          <h2>Login Form</h2>
          <form class="login">
            <div class="form-group">
              <label for="exampleInputEmail1">Email address</label>
              <input type="email" class="form-control" id="email-input" placeholder="Email">
            </div>
            <div class="form-group">
              <label for="exampleInputPassword1">Password</label>
              <input type="password" class="form-control" id="password-input" placeholder="Password">
            </div>
            <button type="submit" class="btn btn-default">Login</button>
          </form>
          <br />
          <p>Or sign up
            <a class="waves-effect waves-light btn modal-trigger" href="#signUp">Sign Up</a>
          </p>
        </div>
      </div>
    </div>
  <div class="modal-footer">
    <a href="/" class="modal-action modal-close waves-effect waves-green btn-flat">Agree</a>
  </div>
</div>

    <!-- Modal Trigger -->
    <a class="waves-effect waves-light btn modal-trigger" href="#signUp">Sign Up</a>
    
    <!-- Modal Structures -->
    <div id="signUp" class="modal">
      <div class="modal-content">
        <h4>Login Header</h4>
        <p>A bunch of text</p>
      </div>
    
      <div class="container">
        <div class="row">
          <div class="col-md-6 col-md-offset-3">
            <h2>Sign Up Form</h2>
            <form class="signup">
              <div class="form-group">
                <label for="exampleInputEmail1">Email address</label>
                <input type="email" class="form-control" id="email-input" placeholder="Email">
              </div>
              <div class="form-group">
                <label for="exampleInputlLicense1">License Plate</label>
                <input type="text" class="form-control" id="license-input" placeholder="License Plate #">
              </div>
              <div class="form-group">
                <label for="exampleInputPassword1">Password</label>
                <input type="password" class="form-control" id="password-input" placeholder="Password">
              </div>
              <div style="display: none" id="alert" class="alert alert-danger" role="alert">
                <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
                <span class="sr-only">Error:</span>
                <span class="msg"></span>
              </div>
              <button type="submit" class="btn btn-default">Sign Up</button>
            </form>
            <br />
            <p>Or log in
              <a href="/login">here</a>
            </p>
          </div>
        </div>
      </div>


  <div class="modal-footer">
    <a href="/" class="modal-action modal-close waves-effect waves-green btn-flat">Agree</a>
  </div>
</div>

